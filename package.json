{
  "name": "shimi",
  "version": "1.3.0",
  "description": "A JS framework for building complex MIDI applications",
  "main": "dist/index.js",
  "unpkg": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamescoyle1989/shimi.git"
  },
  "scripts": {
    "test": "nyc ./node_modules/.bin/_mocha 'test/*.test.ts'",
    "build": "tsc && npx webpack",
    "doc": "npx typedoc",
    "pub": "tsc && npx webpack && npm publish",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "midi",
    "music",
    "toolkit",
    "metronome",
    "chord",
    "gamepad",
    "clip"
  ],
  "author": "James Coyle",
  "license": "MIT",
  "types": "dist/types/index.d.ts",
  "files": [
    "/dist"
  ],
  "devDependencies": {
    "@testdeck/mocha": "0.2.1",
    "@types/chai": "4.3.3",
    "chai": "4.3.6",
    "coveralls": "^3.1.1",
    "mocha": "10.0.0",
    "nyc": "15.1.0",
    "terser-webpack-plugin": "5.3.6",
    "ts-mockito": "2.6.1",
    "ts-node": "10.9.1",
    "typedoc": "0.23.14",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  }
}
